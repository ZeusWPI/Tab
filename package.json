{
  "name": "tab",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.20",
    "@tailwindcss/cli": "^4.1.18",
    "datatables": "^1.10.18",
    "datatables.net": "^2.3.6",
    "esbuild": "^0.27.2",
    "fuse.js": "^7.1.0",
    "luxon": "^3.7.2",
    "mathjs": "^15.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-select": "^5.10.2",
    "tailwindcss": "^4.2.1",
    "tom-select": "^2.4.3"
  },
  "scripts": {
    "build:js": "esbuild ./app/assets/javascript/application.js --loader:.js=jsx --public-path=/assets --outdir=./app/assets/builds/ --bundle --minify",
    "watch:js": "esbuild ./app/assets/javascript/application.js --loader:.js=jsx --public-path=/assets --outdir=./app/assets/builds/ --bundle --watch=forever",
    "build:css": "tailwindcss --input=./app/assets/stylesheets/application.tailwind.css --output=./app/assets/builds/application.css --minify --verbose",
    "watch:css": "tailwindcss --input=./app/assets/stylesheets/application.tailwind.css --output=./app/assets/builds/application.css --verbose --watch=always"
  }
}
