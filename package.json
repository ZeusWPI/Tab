{
  "name": "tab",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.20",
    "@tailwindcss/cli": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "datatables": "^1.10.18",
    "datatables.net": "^2.3.5",
    "esbuild": "^0.27.2",
    "fuse.js": "^7.1.0",
    "luxon": "^3.7.2",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-select": "^5.10.2",
    "tailwindcss": "^4.1.18",
    "tom-select": "^2.4.3"
  },
  "scripts": {
    "build:js": "esbuild ./app/javascript/application.js --loader:.js=jsx --public-path=/assets --bundle --minify --sourcemap --outdir=./app/assets/builds/",
    "build:css": "tailwindcss --input=./app/assets/stylesheets/application.tailwind.css --postcss --minify --verbose --output=./app/assets/builds/application.css",
    "watch:js": "npm run build:js -- --watch=forever",
    "watch:css": "npm run build:css -- --watch=always"
  },
  "devDependencies": {
    "postcss-import": "^16.1.1"
  }
}
